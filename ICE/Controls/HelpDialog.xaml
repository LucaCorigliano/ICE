<toolkit:ToolkitDialog x:Class="Microsoft.Research.ICE.Controls.HelpDialog"                       xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"                       xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"                       xmlns:toolkit="clr-namespace:Microsoft.Research.VisionTools.Toolkit.Desktop;assembly=Microsoft.Research.VisionTools.Toolkit.Desktop"                       xmlns:d="http://schemas.microsoft.com/expression/blend/2008"                       xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"                       Icon="/Images/Window.ico" Title="Image Composite Editor" SizeToContent="WidthAndHeight"                       FocusManager.FocusedElement="{Binding ElementName=okButton}">  <Grid VerticalAlignment="Top" Margin="1,1,1,0">    <Grid.RowDefinitions>      <RowDefinition Height="175"/>      <RowDefinition Height="20"/>      <RowDefinition Height="Auto"/>      <RowDefinition Height="20"/>      <RowDefinition Height="Auto"/>      <RowDefinition Height="10"/>    </Grid.RowDefinitions>    <Grid Width="594" RowSpan="2" Height="175" HorizontalAlignment="Center" VerticalAlignment="Top">      <Image Source="/ICE;component/Images/AboutBanner.png" Stretch="Fill" Width="594" Height="154"             VerticalAlignment="Top" HorizontalAlignment="Center"/>            <Image Margin="20,15,0,87" Source="/ICE;component/Images/LogoICE.PNG" Stretch="Fill" Width="126"             Height="44" HorizontalAlignment="Left" VerticalAlignment="Top"/>            <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Bottom" Margin="20,0,0,2">                        <Hyperlink Command="GoToPage" CommandParameter="https://pixabay.com/users/jplenio-7645255/?utm_source=link-attribution&amp;utm_medium=referral&amp;utm_campaign=image&amp;utm_content=4487709">          <Run Text="jplenio (Pixabay)"/>        </Hyperlink>        <Run Text=": Alps"/>            </TextBlock>        </Grid>    <Grid Row="2" Margin="20,0">      <Grid.ColumnDefinitions>        <ColumnDefinition Width="*"/>        <ColumnDefinition Width="20"/>        <ColumnDefinition Width="*"/>      </Grid.ColumnDefinitions>      <toolkit:BannerButton Content="ICE on GitHub" IconBrush="{DynamicResource IconHomePageBrush}" Command="GoToPage"                            CommandParameter="https://github.com/LucaCorigliano/ICE" Margin="0" Width="Auto"/>        </Grid>    <TextBlock Grid.Row="4" Margin="20,0">      <Run>        Image Composite Editor      </Run>      <LineBreak/>      <Run Text="{Binding Version, FallbackValue='Unable to fetch software Version'}"/>        <LineBreak/>        <Hyperlink Command="GoToPage" CommandParameter="https://www.flaticon.com/free-icons/ice">            <Run Text="Ice icons created by Freepik - Flaticon"></Run>        </Hyperlink>        </TextBlock>    <Button Click="OKButton_Click" x:Name="okButton" Content="OK" IsDefault="true" IsCancel="true" Width="75"            Grid.Row="4" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="20,0"/>  </Grid></toolkit:ToolkitDialog>