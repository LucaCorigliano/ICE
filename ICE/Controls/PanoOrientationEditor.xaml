<panoViewing:PanoViewer x:Class="Microsoft.Research.ICE.Controls.PanoOrientationEditor"                        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"                        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"                        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"                        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"                        xmlns:panoViewing="clr-namespace:Microsoft.Research.ICE.PanoViewing;assembly=StitchEngine"                        xmlns:controls="clr-namespace:Microsoft.Research.ICE.Controls"                        xmlns:D3DCore="clr-namespace:Microsoft.Research.D3DCore;assembly=StitchEngine">  <FrameworkElement.Resources>    <Cursor x:Key="HorizontalCursor">      /Cursors/horizontal.cur    </Cursor>    <Cursor x:Key="VerticalCursor">      /Cursors/vertical.cur    </Cursor>    <Cursor x:Key="TopLeftCursor">      /Cursors/topleft.cur    </Cursor>    <Cursor x:Key="TopRightCursor">     /Cursors/topright.cur    </Cursor>    <Cursor x:Key="BottomLeftCursor">     /Cursors/bottomleft.cur    </Cursor>    <Cursor x:Key="BottomRightCursor">        /Cursors/bottomright.cur    </Cursor>  </FrameworkElement.Resources>  <D3DCore:D3DElement.Overlay>    <Grid>      <controls:GridLineRenderer x:Name="gridLineRenderer"/>      <Grid MouseLeftButtonDown="YawPitchRollArea_MouseLeftButtonDown" Background="Transparent"            UseLayoutRounding="true" SnapsToDevicePixels="true">        <Grid.ColumnDefinitions>          <ColumnDefinition Width="*"/>          <ColumnDefinition Width="8*"/>          <ColumnDefinition Width="*"/>        </Grid.ColumnDefinitions>        <Grid.RowDefinitions>          <RowDefinition Height="*"/>          <RowDefinition Height="8*"/>          <RowDefinition Height="*"/>        </Grid.RowDefinitions>        <Polygon MouseLeftButtonDown="YawPitchRollArea_MouseLeftButtonDown" x:Name="pitchArea" Grid.Column="1"                 Grid.Row="1" Fill="Transparent" Stretch="Fill" Points="0,0 1,0 0,1 1,1"                 Cursor="{DynamicResource VerticalCursor}"/>        <Polygon MouseLeftButtonDown="YawPitchRollArea_MouseLeftButtonDown" x:Name="yawArea" Grid.Column="1"                 Grid.Row="1" Fill="Transparent" Stretch="Fill" Points="0,0 0,1 1,0 1,1"                 Cursor="{DynamicResource HorizontalCursor}"/>      </Grid>    </Grid>    </D3DCore:D3DElement.Overlay></panoViewing:PanoViewer>